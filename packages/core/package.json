{
  "name": "@surimi/core",
  "version": "0.1.0",
  "private": false,
  "author": "Janis jansen <oss@janis.me>",
  "type": "module",
  "imports": {
    "#*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*.js"
    ]
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "format": "prettier . --write",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@surimi/common": "workspace:*",
    "@surimi/parsers": "workspace:*",
    "postcss": "catalog:prod",
    "ts-mixer": "^6.0.4"
  },
  "devDependencies": {
    "@surimi/linter-config": "workspace:*",
    "@surimi/typescript-config": "workspace:*",
    "tsdown": "catalog:build",
    "typescript": "catalog:build"
  }
}
