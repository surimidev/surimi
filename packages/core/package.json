{
  "name": "@surimi/core",
  "version": "0.7.0",
  "private": false,
  "author": "Janis jansen <oss@janis.me>",
  "type": "module",
  "imports": {
    "#*": "./src/*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "format": "prettier . --write",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@surimi/common": "workspace:*",
    "@surimi/parsers": "workspace:*",
    "csstype": "catalog:prod",
    "postcss": "catalog:prod",
    "ts-mixer": "^6.0.4"
  },
  "devDependencies": {
    "@surimi/linter-config": "workspace:*",
    "@surimi/typescript-config": "workspace:*",
    "tsdown": "catalog:build",
    "typescript": "catalog:build"
  }
}
