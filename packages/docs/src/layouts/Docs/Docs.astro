---
import Sidebar from '#components/_docs/Sidebar/Sidebar.astro';

import Main from '../Main/Main.astro';

import './Docs.css';

const { frontmatter } = Astro.props;
const { title } = frontmatter;
---

<Main class="docs" title={title}>
  <Sidebar />
  <main class="docs__content">
    <slot />
  </main>
</Main>
