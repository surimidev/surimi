{
  "name": "@surimi/parsers",
  "version": "0.1.0",
  "description": "A neat little parser for CSS selectors - written in typescript types",
  "keywords": [
    "css",
    "surimi",
    "css-parser",
    "css-selector"
  ],
  "license": "MIT",
  "author": "Janis jansen <oss@janis.me>",
  "type": "module",
  "imports": {
    "#*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts",
      "./src/*.js",
      "./src/*/index.js"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "prettier . --write",
    "test": "pnpm -w test --project=parsers",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@surimi/linter-config": "workspace:*",
    "@surimi/typescript-config": "workspace:*",
    "parsel-js": "^1.2.2",
    "tsdown": "catalog:build",
    "typescript": "catalog:build"
  }
}
