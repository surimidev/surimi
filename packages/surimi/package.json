{
  "name": "surimi",
  "version": "0.3.1",
  "private": false,
  "description": "Next generation CSS-preprocessor (using typescript)",
  "keywords": [
    "css",
    "surimi",
    "css-in-js",
    "ts-in-css"
  ],
  "license": "MIT",
  "author": "Janis jansen <oss@janis.me>",
  "type": "module",
  "imports": {
    "#*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts",
      "./src/*.js",
      "./src/*/index.js"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "format": "prettier . --write",
    "prepack": "pnpm -c exec ../../scripts/replace-symlinks.sh",
    "test": "pnpm -w test --project=surimi",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@surimi/parsers": "workspace:*",
    "csstype": "catalog:prod",
    "postcss": "catalog:prod",
    "ts-mixer": "^6.0.4"
  },
  "devDependencies": {
    "@surimi/linter-config": "workspace:*",
    "@surimi/typescript-config": "workspace:*",
    "rolldown": "catalog:build",
    "tsup": "catalog:build",
    "typescript": "catalog:build"
  }
}
