{
  "name": "@surimi/compiler",
  "version": "0.3.1",
  "description": "Compile surimi ts files to css",
  "keywords": [
    "css",
    "surimi",
    "css-in-js"
  ],
  "license": "MIT",
  "author": "Janis Jansen <oss@janis.me>",
  "type": "module",
  "imports": {
    "#*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "surimi": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "test": "pnpm -w test --project=compiler",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "commander": "^14.0.2",
    "rolldown": "catalog:build"
  },
  "devDependencies": {
    "@surimi/linter-config": "workspace:*",
    "@surimi/typescript-config": "workspace:*",
    "@types/node": "catalog:types",
    "tsdown": "catalog:build",
    "typescript": "catalog:build"
  },
  "peerDependencies": {
    "surimi": "workspace:*"
  }
}
