{
  "name": "vite-plugin-surimi",
  "version": "0.1.2",
  "description": "Vite plugin for automatic CSS generation with Surimi",
  "keywords": [
    "vite",
    "plugin",
    "css",
    "surimi",
    "css-in-js"
  ],
  "license": "MIT",
  "author": "Janis jansen <oss@janis.me>",
  "type": "module",
  "imports": {
    "#*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@surimi/compiler": "workspace:*"
  },
  "devDependencies": {
    "@janis.me/linter-config": "workspace:*",
    "@janis.me/typescript-config": "workspace:*",
    "@types/node": "catalog:types",
    "tsdown": "catalog:build",
    "typescript": "catalog:build",
    "vite": "catalog:build"
  },
  "peerDependencies": {
    "surimi": "workspace:*",
    "vite": "catalog:build"
  }
}
