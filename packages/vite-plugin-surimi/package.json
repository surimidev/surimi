{
  "name": "vite-plugin-surimi",
  "version": "0.1.0",
  "description": "Vite plugin for automatic CSS generation with Surimi",
  "keywords": [
    "vite",
    "plugin",
    "css",
    "surimi",
    "css-in-js"
  ],
  "license": "MIT",
  "author": "Janis",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@rollup/pluginutils": "catalog:build",
    "chokidar": "catalog:build",
    "esbuild": "catalog:build",
    "fast-glob": "catalog:build"
  },
  "devDependencies": {
    "@types/node": "catalog:types",
    "tsdown": "catalog:build",
    "typescript": "catalog:build",
    "vite": "catalog:build"
  },
  "peerDependencies": {
    "surimi": "workspace:*",
    "vite": "*7.0.0"
  }
}
